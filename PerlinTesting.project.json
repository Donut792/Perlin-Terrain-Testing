{
  "name": "Perlin-Terrain-Testing",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,
      "SharedModules": {
        "$path": "Source/SharedModules"
      },
      "Modules" : {
        "$path": "Source/ClientModules"
      }
    },
    
    "ServerStorage": {
      "$className": "ServerStorage",
      "$ignoreUnknownInstances": true,
      "Modules": {
        "$path": "Source/ServerStorage"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,
      "Server": {
        "$path": "Source/ServerMain"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$ignoreUnknownInstances": true,
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": {
          "$path": "Source/StarterPlayerScripts/Client"
        },
        "Server": {
          "$path": "Source/StarterPlayerScripts/Server"
        }
      },
      "StarterCharacterScripts": {
        "$className": "StarterCharacterScripts",
        "Client": {
          "$path": "Source/StarterCharacterScripts/Client"
        },
        "Server": {
          "$path": "Source/StarterCharacterScripts/Server"
        }
      }
    }
  }
}